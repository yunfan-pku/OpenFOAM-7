FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
      volScalarFieldValue T 293.0
      volScalarFieldValue p 1e+5
    //volScalarFieldValue Ydefault 0.9
    //volScalarFieldValue CO2 0.850829
      volScalarFieldValue N2 1e-5
      volScalarFieldValue H2O 0.99999
      volVectorFieldValue U (0 0 0)
);

regions
(
    boxToCell
    {
        //box (40.e-06 0 0) (104.e-06 1 1);
        //box (0.5e-4 0 0) (1e-04 1 1);
	box (0.48 0 0) (0.52 1 1);
        fieldValues
        (
            volScalarFieldValue T 293.0
            volScalarFieldValue p 1.01e+5  
            //volScalarFieldValue Ydefault 0.9
            //volScalarFieldValue CO2 0.850829
            //volScalarFieldValue CH4 0.149171
            volScalarFieldValue N2 1e-5
            volScalarFieldValue H2O 0.99999

            volVectorFieldValue U (0 0 0)
        );
    }
);


